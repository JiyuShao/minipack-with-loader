{
  "name": "minipack-with-loader",
  "version": "0.0.1",
  "description": "add loader logic for minipack",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "npm run lint && rm -rf lib && tsc",
    "lint": "npm run lint:es && npm run lint:prettier",
    "lint:es": "eslint --ext .ts ./ --cache --quiet",
    "lint:prettier": "prettier --list-different \"**/*.{ts,json,yml,yaml,md}\"",
    "fix": "npm run fix:es && npm run fix:prettier",
    "fix:es": "eslint --ext .ts ./ --cache --quiet --fix",
    "fix:prettier": "prettier --write \"**/*.{ts,json,yml,yaml,md}\"",
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "watch": "jest --watch"
  },
  "keywords": [
    "minipack",
    "loader"
  ],
  "author": "Jiyu Shao",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.6",
    "prettier": "^2.0.4",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.9.5",
    "dedent": "^0.7.0"
  }
}
